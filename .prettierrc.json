{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 120,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "proseWrap": "always",
  "jsxSingleQuote": true,
  "quoteProps": "consistent",
  "endOfLine": "auto",
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "printWidth": 80
      }
    }
  ]
}